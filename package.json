    "tsyringe": "^4.4.0",
    "uuid": "^8.3.2",
    "validatorjs": "^3.22.1",
    "zod": "^3.22.4"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "eslintIgnore": [
    "*.test.ts"
  ],
  "lint-staged": {
    "**/*.{js, ts}": [
      "npm run lint -- --fix",
      "npx prettier --write ./src"
    ]
  }
}
