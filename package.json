{
  "name": "restana.template",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "dist/src/server.js",
  "author": "imadu <imadu@gmail.com>",
  "keywords": [],
  "scripts": {
    "start:dev": "nodemon",
    "start": "node dist/server.js",
    "build": "tsc",
    "gcp-build": "npm run build && touch dist/.env",
    "postbuild": "copyfiles -u 1 \"src/**/*.yaml\" dist",
    "prepare": "husky install",
    "knex": "ts-node -r dotenv/config ./node_modules/knex/bin/cli.js",
    "db:migrate:make": "npm run knex -- migrate:make",
    "db:migrate": "npm run knex -- migrate:latest",
    "db:migrate:rollback": "npm run knex -- migrate:rollback",
    "db:seed": "yarn run knex -- seed:run",
    "lint": "npx prettier --write . && eslint --ignore-path .eslintignore --ext .ts .",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/chai-as-promised": "^7.1.6",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.8.8",
    "@types/validatorjs": "^3.15.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.1",
    "btoa": "^1.2.1",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "compression": "^1.7.4",
    "crypto-js": "^4.1.1",
    "customerio-node": "4.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.0",
    "event-dispatch": "^0.4.1",
    "firebase-admin": "^11.11.0",
    "http-proxy-middleware": "^2.0.6",
    "http-status": "^1.5.1",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.0.4",
    "joi": "^17.6.0",
    "js-sha256": "^0.9.0",
    "js-sha512": "^0.8.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.7",
    "lint-staged": "^13.0.3",
    "middleware-if-unless": "^1.4.0",
    "module-alias": "^2.2.2",
    "ms": "^2.1.3",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "pino": "^7.10.0",
    "pino-http": "^8.2.1",
    "qs": "^6.11.0",
